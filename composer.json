{
    "name": "medicalmundi/access-gudid-http-client",
    "type": "library",
    "license": "MIT",
    "description": "Http consumer for Access Gudid Api.",
    "keywords": ["access-gudid", "implantable-device"],
    "authors": [
        {"name": "Teclai Zerai", "email": "teclaizerai@gmail.com"},
        {"name": "Francesca Bonadonna", "email": "francescabonadonna@medicalmundi.com"}
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "nyholm/psr7": "^1.2",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/http-client": "^4.4||^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MedicalMundi\\AccessGudid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MedicalMundi\\AccessGudid\\Tests\\": "tests/"
        }
    }
}
