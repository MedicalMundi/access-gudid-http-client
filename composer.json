{
    "name": "medicalmundi/php-access-gudid-consumer",
    "type": "library",
    "description": "Http consumer for Access Gudid Api.",
    "keywords": [
        "healthcare",
        "medical",
        "medical-informatics",
        "access-gudid",
        "implantable-device",
        "api-consumer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Teclai Zerai",
            "email": "teclaizerai@gmail.com"
        },
        {
            "name": "Francesca Bonadonna",
            "email": "francescabonadonna@medicalmundi.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "nyholm/psr7": "^1.7",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/http-client": "^4.4 || ^5.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.5",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        }
    },
    "autoload": {
        "psr-4": {
            "MedicalMundi\\AccessGudid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MedicalMundi\\AccessGudid\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
