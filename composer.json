{
    "name": "medicalmundi/access-gudid-http-client",
    "type": "library",
    "license": "MIT",
    "description": "Http client for Access Gudid api.",
    "keywords": ["access-gudid", "implantable-device"],
    "authors": [
        {"name": "Teclai Zerai", "email": "teclaizerai@gmail.com"},
        {"name": "Francesca Bonadonna", "email": "francescabonadonna@medicalmundi.com"}
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/psr7": "^1.0",
        "php-http/message": "^1.0",
        "php-http/mock-client": "^1.0",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MedicalMundi\\AccessGudid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MedicalMundi\\AccessGudid\\Tests\\": "tests/"
        }
    }
}
